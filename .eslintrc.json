{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "object-curly-newline": "warn",
    "comma-dangle": "warn",
    "arrow-body-style": "warn",
    "operator-linebreak": "warn",
    "no-empty": "warn"
  }
}
